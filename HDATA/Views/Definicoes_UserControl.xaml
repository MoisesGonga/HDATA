                                <Label Content="Email" HorizontalAlignment="Left"  VerticalAlignment="Center" Grid.Column="3" Grid.Row="10" Height="28"  Margin="10,0,10,0" FontWeight="Normal" />
                                <TextBox x:Name="txt_email_admin"    Grid.Row="11" Grid.Column="3" Margin="10,0,25,0" FontWeight="Normal" VerticalContentAlignment="Center" />



